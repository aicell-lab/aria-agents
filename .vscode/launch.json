{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Connect to remote",
            "type": "debugpy",
            "request": "launch",
            "module": "aria_agents",
            "justMyCode": false,
            "env": {
                "JWT_SECRET": "1337",
                "OPENAI_API_MODEL": "gpt-4o-2024-08-06",
                "BIOIMAGEIO_DEBUG": "true",
            },
            "args": [
                "remote",
                "--server-url=https://hypha.aicell.io",
                "--login-required"
            ]
        },
        {
            "name": "Run locally",
            "type": "debugpy",
            "request": "launch",
            "module": "aria_agents",
            "justMyCode": false,
            "preLaunchTask": "Start MinIO",
            "env": {
                "JWT_SECRET": "1337",
                "OPENAI_API_MODEL": "gpt-4o-2024-08-06",
                "BIOIMAGEIO_DEBUG": "true",
            },
            "args": [
                "local",
                "--port=9527",
                "--login-required"
            ]
        },
    ]
}